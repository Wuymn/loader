-- Script
Tab:AddTextbox({
	Name = "Key",
	Default = "Enter Key",
	TextDisappear = true,
	Callback = function(Value)
  _G.KeyInput = Value	
	end	  
})

Tab:AddButton({
	Name = "Check Key",
	Callback = function()
         if _G.KeyInput == _G.CurrentKey then
  LoadScript()
          end
  	end    
})

OrionLib:Init()
